{
	"name": "@tokilabs/nes",
	"version": "0.0.1",
	"description": "Event sourcing module for Nest framework (node.js)",
	"license": "MIT",
	"contributors": [
		{
			"name": "Saulo Vallory",
			"email": "saulo@toki.life"
		},
		{
			"name": "Yago Fernandes",
			"email": "yago@toki.life"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tokilabs/nes"
	},
	"scripts": {
		"test": "jest",
		"test:watch": "jest --watch",
		"build": "rm -rf ./dist/ && tsc -p tsconfig.json",
		"build:watch": "rm -rf ./dist/ && tsc -w -p tsconfig.json",
		"format": "prettier --write \"**/*.ts\"",
		"lint": "eslint \"src/**/*.ts\" --fix",
		"prepublish:next": "npm run build",
		"publish:next": "npm publish --access public --tag next",
		"prepublish:npm": "npm run build",
		"publish:npm": "npm publish --access public",
		"prerelease": "npm run build",
		"release": "release-it"
	},
	"dependencies": {
		"@tokilabs/lang": "^1.0.3",
		"class-transformer": "^0.4.0",
		"class-validator": "^0.13.1",
		"servicebus": "^2.3.3"
	},
	"devDependencies": {
		"@commitlint/cli": "12.1.4",
		"@commitlint/config-angular": "12.1.4",
		"@nestjs/common": "7.6.17",
		"@nestjs/core": "7.6.17",
		"@types/amqplib": "^0.8.0",
		"@types/jest": "26.0.23",
		"@types/node": "14.17.1",
		"@types/redis": "^2.8.29",
		"@typescript-eslint/eslint-plugin": "4.25.0",
		"@typescript-eslint/parser": "4.25.0",
		"eslint": "7.27.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-import": "2.23.3",
		"eslint-plugin-tsdoc": "^0.2.14",
		"husky": "6.0.0",
		"jest": "26.6.3",
		"lint-staged": "11.0.0",
		"prettier": "2.3.0",
		"reflect-metadata": "0.1.13",
		"release-it": "14.7.0",
		"rxjs": "^7.1.0",
		"ts-jest": "26.5.6",
		"typescript": "4.3.2"
	},
	"peerDependencies": {
		"@nestjs/common": "^6.0.0 || ^7.0.0",
		"@nestjs/core": "^6.0.0 || ^7.0.0",
		"reflect-metadata": "0.1.13",
		"rxjs": "^6.4.0"
	},
	"optionalDependencies": {
		"amqplib": "^0.8.0",
		"node-eventstore-client": "^0.1.7",
		"redis": "^3.1.2"
	},
	"lint-staged": {
		"*.ts": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
			"pre-commit": "lint-staged"
		}
	}
}

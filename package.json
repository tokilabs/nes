{
	"name": "@tokilabs/nestjs-event-sourcing",
	"version": "0.0.6 ",
	"description": "Event sourcing module for Nest framework (node.js)",
	"license": "MIT",
	"contributors": [
		{
			"name": "Saulo Vallory",
			"email": "saulo@toki.life"
		},
		{
			"name": "Yago Fernandes",
			"email": "yago@toki.life"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tokilabs/nestjs-event-sourcing"
	},
	"scripts": {
		"test": "jest",
		"test:watch": "jest --watch",
		"build": "rm -rf ./dist/ && tsc -p tsconfig.json",
		"build:watch": "rm -rf ./dist/ && tsc -w -p tsconfig.json",
		"format": "prettier --write \"**/*.ts\"",
		"lint": "eslint \"src/**/*.ts\" --fix",
		"prepublish:next": "npm run build",
		"publish:next": "npm publish --access public --tag next",
		"prepublish:npm": "npm run build",
		"publish:npm": "npm publish --access public",
		"prerelease": "npm run build",
		"release": "release-it"
	},
	"dependencies": {
		"@tokilabs/lang": "^1.0.7",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"servicebus": "^2.3.3"
	},
	"devDependencies": {
		"@commitlint/cli": "17.6.1",
		"@commitlint/config-angular": "17.6.1",
		"@nestjs/common": "9.4.0",
		"@nestjs/core": "9.4.0",
		"@types/amqplib": "^0.10.1",
		"@types/jest": "29.5.1",
		"@types/node": "18.16.3",
		"@types/redis": "^4.0.11",
		"@typescript-eslint/eslint-plugin": "5.59.2",
		"@typescript-eslint/parser": "5.59.2",
		"eslint": "8.39.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-tsdoc": "^0.2.17",
		"husky": "8.0.3",
		"jest": "29.5.0",
		"lint-staged": "13.2.2",
		"prettier": "2.8.8",
		"reflect-metadata": "0.1.13",
		"release-it": "15.10.3",
		"rxjs": "^7.8.1",
		"ts-jest": "29.1.0",
		"typescript": "5.0.4"
	},
	"peerDependencies": {
		"@nestjs/common": "^8.0.0 || ^9.0.0",
		"@nestjs/core": "^8.0.0 || ^9.0.0",
		"reflect-metadata": "0.1.13",
		"rxjs": "^7.8.1"
	},
	"optionalDependencies": {
		"amqplib": "^0.10.3",
		"node-eventstore-client": "^0.1.7",
		"redis": "^4.6.6"
	},
	"lint-staged": {
		"*.ts": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
			"pre-commit": "lint-staged"
		}
	}
}
